button.button.is-success.system-button.system-button-top-left.slide-down-animation('(click)'="refreshItems()") More üèÜ

.view.view--timeline
  .timeline.is-centered

    ng-container('*ngFor'="let dead of getItems()")
      ng-container('*ngIf'="dead.type==='archive'")
        .timeline-item
          .timeline-marker
          .timeline-content
            h2 {{dead.age}}
            p.timeline--desc {{dead.desc}}

      ng-container('*ngIf'="dead.type==='terminator'")
        ng-container('*ngIf'="dead.age!==0")
          .timeline-item
            .timeline-content
          .timeline-item
            .timeline-content
        header.timeline-header
          span.tag.is-medium.is-danger {{dead.desc}}
        ng-container('*ngIf'="dead.age===0")
          .timeline-item
            .timeline-content
          .timeline-item
            .timeline-content

      ng-container('*ngIf'="dead.type==='event'")
        .timeline-item
          .timeline-content
        .timeline-item
          .timeline-content
        header.timeline-header
          span.tag.is-warning {{dead.desc}}
        ng-container('*ngIf'="age < lifeExpectancy || dead.age !== age")
          .timeline-item
            .timeline-content
          .timeline-item
            .timeline-content

